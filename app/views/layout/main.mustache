	<link rel="stylesheet" href="/css/cards.css">
	
	<script src="/js/lib/dialog-polyfill/dialog-polyfill.js"></script>
	<link rel="stylesheet" href="/css/lib/dialog-polyfill/dialog-polyfill.css">
